function accionesDisponibles(caballero, arquero, prisionero, perro) {
    const acciones = {
      ataqueRapido: false,
      espia: false,
      senalarPrisionero: false,
      prisioneroLibre: false,
    };
  
    // Ataque r√°pido
    if (caballero === "dormido") {
      acciones.ataqueRapido = true;
    }
  
    // Espiar
    if (caballero === "despierto" || arquero === "despierto") {
      acciones.espia = true;
    }



}
