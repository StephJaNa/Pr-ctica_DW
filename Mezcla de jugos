//Mezcla de jugos

//1.Menu

function timeToMixJuice(jugo){
    switch(jugo){
        case 'Pure Strawberry Joy':
            return 0.5;
        case 'Energizer':
            return 1.5;
        case 'Green Garden':
            return 1.5;
        case 'Tropical Island':
            return 3.0;
        case 'All or nothing':
            return 5.0;
        default:
            return 2.5;
    }
}
